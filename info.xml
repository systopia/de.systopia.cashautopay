<?xml version="1.0"?>
<extension key="assumed-payments" type="module">
  <file>assumed_payments</file>
  <name>Assumed Payments</name>
  <description>Assumed payments extension to generate and process recurring contributions.</description>
  <license>AGPL-3.0-or-later</license>
  <maintainer>
    <author>ortiz</author>
    <email>ortiz@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/assumed-payments</url>
    <url desc="Documentation">https://docs.civicrm.org/assumed-payments</url>
    <url desc="Support">https://github.com/systopia/assumed-payments/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.0.0-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>6.1</ver>
  </compatibility>
  <comments>This extension allows to create recurring contributions and payments for configured payment instruments.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/AssumedPayments</namespace>
    <format>25.10.1</format>
    <angularModule>crmAssumedPayments</angularModule>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
</extension>
